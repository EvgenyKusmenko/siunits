package de.monticore.types;

/**
 * This grammar interprets Primitive with SIUnits as types to distinguish between the type
 * 'double m', e.g. in a declaration and '3.2 m' in an expression.
*/

component grammar PrimitiveWithSIUnitTypes extends de.monticore.types.MCBasicTypes, de.monticore.types.SIUnitTypes {

    PrimitiveWithSIUnitType implements MCObjectType =
        "<" MCPrimitiveType SIUnitType ">";

}