package de.monticore.types;

/**
 * This grammar interprets the SIUnits as types to distinguish between the type 'm', e.g. in a declaration
 * and 'm' in an expression.
*/

component grammar SIUnitTypes extends de.monticore.lang.siunits.SIUnits,
        de.monticore.types.MCBasicTypes {

     SIUnitType implements MCObjectType =
        SIUnit;
}