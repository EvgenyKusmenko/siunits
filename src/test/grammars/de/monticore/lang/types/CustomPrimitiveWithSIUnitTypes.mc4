package de.monticore.lang.types;

/**
 * This grammar is another way to represent the type of a primitive combined with a SI unit type.
 */

component grammar CustomPrimitiveWithSIUnitTypes extends de.monticore.lang.types.PrimitiveWithSIUnitTypes {

    CustomPrimitiveWithSIUnitType implements PrimitiveWithSIUnitTypeInt =
        primitiveType:MCPrimitiveType "<" siunitType:SIUnitType ">";
}